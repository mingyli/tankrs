// The definition for the base game state.

struct MyVec {
    a: float;
    b: float;
}

table Block {
    x: float;
    v: MyVec;
}

table World {
    // Dimensions of the world.
    width: uint16;
    height: uint16;

    block: Block;
}

union Thing {
    World
}

table Message {
    thing: Thing;
}

root_type Message;

